{
  "name": "vue-library-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend": "json-server --watch db.json --routes routes.json --port 3000 --delay 250 --middlewares ./node_modules/json-server-reset",
    "dev": "concurrently \"npm run backend\" \"npm run serve\""
  },
  "dependencies": {
    "awesome-notifications": "^3.0.3",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "concurrently": "^5.0.0",
    "core-js": "^3.4.2",
    "fecha": "^3.0.3",
    "jquery": "^3.4.1",
    "lodash-es": "^4.17.15",
    "popper.js": "^1.16.0",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "json-server": "^0.15.1",
    "json-server-reset": "^1.1.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
